{
  "name": {
    "en": "TP-Link Managed Switch"
  },
  "class": "networkrouter",
  "capabilities": [
    "onoff.favorite",
    "onoff.leds"
  ],
  "capabilitiesOptions": {
    "onoff.favorite": {
      "title": {
        "en": "Favorite Port"
      },
      "uiQuickAction": false
    },
    "onoff.leds": {
      "title": {
        "en": "LEDs"
      },
      "uiQuickAction": false
    }
  },
  "platforms": [
    "local"
  ],
  "connectivity": [
    "lan"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png",
    "xlarge": "{{driverAssetsPath}}/images/xlarge.png"
  },
  "pair": [
    {
      "id": "connection_input"
    },
    {
      "id": "loading",
      "template": "loading"
    },
    {
      "id": "connection_error"
    },
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": { "next": "add_devices" }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    },
    {
      "id": "done",
      "template": "done"
    }
  ],
  "repair": [
    {
      "id": "connection_input"
    },
    {
      "id": "loading",
      "template": "loading"
    },
    {
      "id": "connection_error"
    },
    {
      "id": "incorrect_device_error"
    },
    {
      "id": "done",
      "template": "done"
    }
  ]
}
